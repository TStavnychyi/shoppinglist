<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/msla_common_button_disabled_state" />
            <stroke android:width="1dp" android:color="@color/msla_common_button_disabled_state" />
            <corners android:radius="36dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/msla_common_button_enabled_state" />
            <corners android:radius="36dp" />
        </shape>
    </item>

</selector>